									"</style>",
									"",
									"<table class=\"tftable\" border=\"1\">",
									"    <tr>",
									"        <th>Comment Content</th>",
									"        <th>Event ID</th>",
									"        <th>Member ID</th>",
									"    </tr>",
									"    ",
									"    {{#each response}}",
									"        <tr>",
									"            <td>{{contenu_commentaire}}</td>",
									"            <td>{{id_evenement}}</td>",
									"            <td>{{id_membre}}</td>",
									"        </tr>",
									"    {{/each}}",
									"</table>",
									"`;",
									"",
									"function constructVisualizerPayload() {",
									"    return { response: pm.response.json() };",
									"}",
